{
  "name": "spryker/elastica",
  "type": "library",
  "description": "Elastica module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "ruflin/elastica": "^6.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
